<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0px" android:color="#ff000000" />
            <corners android:radius="40.0dip" />
            <padding android:left="10.0dip" />
            <padding android:right="10.0dip" />
            <padding android:top="10.0dip" />
            <padding android:bottom="10.0dip" />
            <gradient android:startColor="#ffc79999" android:endColor="#ffc72929" android:angle="270.0" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <stroke android:width="3.0px" android:color="#ffc72929" />
            <corners android:radius="40.0dip" />
            <padding android:left="10.0dip" />
            <padding android:right="10.0dip" />
            <padding android:top="10.0dip" />
            <padding android:bottom="10.0dip" />
            <gradient android:startColor="#ffffffff" android:endColor="#ffacacac" android:angle="270.0" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <stroke android:width="1.0px" android:color="#ff000000" />
            <corners android:radius="40.0dip" />
            <padding android:left="10.0dip" android:top="10.0dip" android:right="10.0dip" android:bottom="10.0dip" />
            <gradient android:startColor="#ffe6302c" android:endColor="#ffe6302c" android:angle="270.0" />
            <corners android:radius="40.0dip" />
        </shape>
    </item>
</selector>